https://tecadmin.net/laravel-file-permissions/
sudo chown -R www-data:www-data /la8ui

cd /var/www/vhosts/rutasgp.com/httpdocs/la8ui

sudo chown -R www-data:www-data /var/www/vhosts/rutasgp.com/httpdocs/la8ui

sudo find /var/www/vhosts/rutasgp.com/httpdocs/la8ui -type f -exec chmod 644 {} \;
sudo find /var/www/vhosts/rutasgp.com/httpdocs/la8ui -type d -exec chmod 755 {} \;


sudo find /path/to/laravel -type f -exec chmod 644 {} \;
sudo find /path/to/laravel -type d -exec chmod 755 {} \;


cd /path/to/laravel
sudo chgrp -R www-data storage bootstrap/cache
sudo chmod -R ug+rwx storage bootstrap/cache

sudo usermod -a -G www-data vnt
